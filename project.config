[project]
	description = Ceph is a distributed object, block, and file storage platform 
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/CHEN TAO (taoCH)
[access "refs/for/refs/*"]
	read = group user/CHEN TAO (taoCH)
	push = group user/CHEN TAO (taoCH)
	label-Code-Review = -2..+2 group user/CHEN TAO (taoCH)
	label-Verified = -1..+1 group user/CHEN TAO (taoCH)
	rebase = group user/CHEN TAO (taoCH)
	removeReviewer = group user/CHEN TAO (taoCH)
	submit = group user/CHEN TAO (taoCH)
[access "refs/heads/*"]
	read = group user/CHEN TAO (taoCH)
	create = group user/CHEN TAO (taoCH)
	push = +force group user/CHEN TAO (taoCH)
	pushMerge = group user/CHEN TAO (taoCH)
[access "refs/tags/*"]
	push = group user/CHEN TAO (taoCH)
	push = +force group user/CHEN TAO (taoCH)
